{
  "name": "faire",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "eslint-plugin-react": "^7.21.2",
    "gh-pages": "^3.1.0",
    "node-sass-chokidar": "^1.3.5"
  },
  "dependencies": {
    "@craco/craco": "^5.7.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-modal": "^3.10.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.4",
    "uikit": "^3.2.1",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-react": "craco start",
    "prebuild": "npm run build-css",
    "build": "craco build",
    "eject": "react-scripts eject",
    "test": "craco test --env=jsdom",
    "start": "npm run watch-css | npm run start-react",
    "build-css": "node-sass-chokidar --output-style compressed --include-path ./node_modules/ ./src/styles/scss/index.scss ./src/styles/index.css",
    "watch-css": "node-sass-chokidar --output-style compressed --include-path ./node_modules/ ./src/styles/scss/index.scss ./src/styles/index.css --watch --recursive"
  },
  "homepage": "https://nishanbajracharya.github.io/faire/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
