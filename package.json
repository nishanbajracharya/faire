{
  "name": "faire",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "node-sass-chokidar": "^1.3.5"
  },
  "dependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-modal": "^3.10.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.4",
    "uikit": "^3.2.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-react": "react-scripts start",
    "prebuild": "npm run build-css",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom",
    "start": "npm run watch-css | npm run start-react",
    "build-css": "node-sass-chokidar --output-style compressed --include-path ./node_modules/ ./src/styles/scss/index.scss ./src/styles/index.css",
    "watch-css": "node-sass-chokidar --output-style compressed --include-path ./node_modules/ ./src/styles/scss/index.scss ./src/styles/index.css --watch --recursive"
  },
  "homepage": "https://nishanbajracharya.github.io/faire/"
}
